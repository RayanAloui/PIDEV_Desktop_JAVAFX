<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700" prefWidth="900" stylesheets="@Style/styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="esprit.tn.controllers.CoursListController">

    <TableView fx:id="tableCours" layoutX="50.0" layoutY="89.0" prefHeight="400.0" prefWidth="800.0">
