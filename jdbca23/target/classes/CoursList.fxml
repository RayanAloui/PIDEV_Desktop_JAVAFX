            <TableColumn fx:id="colTitre" text="Titre" />
            <TableColumn fx:id="colContenu" prefWidth="172.79995727539062" text="Contenu" />
            <TableColumn fx:id="colResume" prefWidth="181.60006713867188" text="Résumé" />
            <TableColumn fx:id="colTuteur" prefWidth="146.39996337890625" text="Tuteur" />
            <TableColumn fx:id="colImage" prefWidth="196.79998779296875" text="Image" />
        </columns>
    </TableView>

    <Button fx:id="btnModifierCours" layoutX="390.0" layoutY="521.0" onAction="#ouvrirFenetreModification" text="Modifier" />
   <Button layoutX="630.0" layoutY="521.0" mnemonicParsing="false" onAction="#supprimerCours" text="Supprimer " />
   <Button layoutX="150.0" layoutY="521.0" mnemonicParsing="false" onAction="#AjouterCours" text="Ajouter" />
   <Button layoutX="270.0" layoutY="584.0" mnemonicParsing="false" onAction="#AfficherTuteurs" text="Afficher Tuteurs" />
   <Button layoutX="510.0" layoutY="584.0" mnemonicParsing="false" onAction="#AfficherOrphelins" text="AfficherOrphelin" />


</AnchorPane>
