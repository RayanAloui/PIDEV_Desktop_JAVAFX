    <children>
        <VBox alignment="CENTER" layoutX="305.0" layoutY="189.0" prefHeight="322.0" prefWidth="290.0" spacing="10.0">
            <Label text="New Password:" />
            <PasswordField fx:id="newPasswordField" />

            <Label text="Confirm Password:" />
            <PasswordField fx:id="confirmPasswordField" />

            <Button fx:id="okButton" onAction="#handleOkButton" text="OK" />
        </VBox>
      <Label fx:id="label" prefHeight="150.0" prefWidth="321.0" text="Label" />
    </children>
</AnchorPane>
